@{
    ViewData["Title"] = "Employees";
}
<div class="row">
    <div class="col-md-12">
        <a asp-action="Create" asp-controller="Employee" class="btn btn-sucess">Fill Form</a>
    </div>
</div>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Code</th>
      <th scope="col">Initails</th>
      <th scope="col">FName</th> 
      <th scope="col">Surname</th> 
      <th scope="col">Address1 </th> 
      <th scope="col">Address2 </th> 
      <th scope="col">DB</th> 
      <th scope="col">Status</th> 
  </thead>
            <tbody>
                @foreach(var item in Model){
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Initials</td>
                    <td>@item.FirstName</td>
                    <td>@item.Surname</td>
                    <td>@item.Address1</td>
                    <td>@item.Address2</td>
                    <td>@item.DOB</td>
                    <td>@item.Status</td>
                </tr>
                }
            </tbody>
</table>

